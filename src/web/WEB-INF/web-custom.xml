<?xml version='1.0' encoding='ISO-8859-1'?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <!-- Servlets -->
    <servlet>
        <servlet-name>VersionServlet</servlet-name>
        <servlet-class>org.tttalk.openfire.plugin.VersionServlet</servlet-class>
    </servlet>

    <!-- Servlet mappings -->
    <servlet-mapping>
        <servlet-name>VersionServlet</servlet-name>
        <url-pattern>/ver</url-pattern>
    </servlet-mapping>
    <!-- Servlets -->
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>org.tttalk.openfire.plugin.LoginServlet</servlet-class>
    </servlet>

    <!-- Servlet mappings -->
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <!-- Servlets -->
    <servlet>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>org.tttalk.openfire.plugin.SearchServlet</servlet-class>
    </servlet>
    <!-- Servlet mappings -->
    <servlet-mapping>
        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>

    <!-- Servlets -->
    <servlet>
        <servlet-name>OrgServlet</servlet-name>
        <servlet-class>org.tttalk.openfire.plugin.OrgServlet</servlet-class>
    </servlet>
    <!-- Servlet mappings -->
    <servlet-mapping>
        <servlet-name>OrgServlet</servlet-name>
        <url-pattern>/org</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServiceServlet</servlet-name>
        <servlet-class>org.tttalk.openfire.plugin.ServiceServlet</servlet-class>
    </servlet>
    <!-- Servlet mappings -->
    <servlet-mapping>
        <servlet-name>ServiceServlet</servlet-name>
        <url-pattern>/service</url-pattern>
    </servlet-mapping>

    <!-- Servlets -->
    <servlet>
        <servlet-name>SendServlet</servlet-name>
        <servlet-class>org.tttalk.openfire.plugin.SendServlet</servlet-class>
    </servlet>
    <!-- Servlet mappings -->
    <servlet-mapping>
        <servlet-name>SendServlet</servlet-name>
        <url-pattern>/send</url-pattern>
    </servlet-mapping>
</web-app>
